---
app:
  target: {{ app.target }}
  debug: {{ app.debug }}
  test_firmware_path: {{ app.test_firmware_path }}
  prod_firmware_path: {{ app.prod_firmware_path }}
  skip_test_firmware_load: {{ app.skip_test_firmware_load }}
device:
  minter_id: {{ '%#04x' % device.minter_id }}
  thing_type_name: {{ device.thing_type_name }}
  thing_type_id: {{ '%#04x' % device.thing_type_id }}
  hw_revision: {{ device.hw_revision }}
  assembly_id: {{ '%#04x' % device.assembly_id }}
  assembly_version: {{ device.assembly_version }}
  manufacturer_name: {{ device.manufacturer_name }}
  manufacturer_id: {{ '%#04x' % device.manufacturer_id }}
  cable_lengths: {{ device.cable_lengths }}
api:
  region: {{ api.region }}
  host: {{ api.host }}
  stage: {{ api.stage }}
lora:
  join_eui: {{ lora.join_eui }}
  app_key: {{ lora.app_key }}
network:
  ping_interval: {{ network.ping_interval }} # in seconds
